{
    "name": "rajagonda/youtube",
    "description": "Upload a video to YouTube with Laravel 5.",
    "license": "MIT",
    "authors": [
        {
            "name": "rajagonda",
            "email": "rajagonda@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/support": "^6.0.2",
        "google/apiclient": "^2.2"
    },
    "require-dev": {
        "orchestra/testbench": "~3.2",
        "phpunit/phpunit": "8.0",
        "phpunit/phpcov": "~6.0"

    },
    "autoload": {
        "psr-4": {
            "Rajagonda\\Youtube\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rajagonda\\Youtube\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Rajagonda\\Youtube\\YoutubeServiceProvider"
            ],
            "aliases": {
                "Youtube": "Rajagonda\\Youtube\\Facades\\Youtube"
            }
        }
    }
}
